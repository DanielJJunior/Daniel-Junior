RT-02

enable 
conf t
username Daniel privilege 15 secret D@nielJuni0r
interface g0/1
no sh
interface g0/1.50
encapsulation dot1Q 50
ip add 10.0.0.1 255.255.128.0
no sh 
description ANALISTAS 
exit
interface g0/1.60
encapsulation dot1Q 60
ip add 10.0.128.1 255.255.224.0
no sh 
description HOST OPERACAO
exit
interface g0/1.70
encapsulation dot1Q 70
ip add 10.0.160.1 255.255.248.0
no sh 
description IPS TI
exit
interface g0/1.80
encapsulation dot1Q 80
ip add 10.0.168.1 255.255.254.0
no sh 
description IPS ADMINISTRACAO
exit
interface serial 0/0/1
ip add 201.187.89.6 255.255.255.252
no sh
interface serial 0/1/1
ip add 201.187.89.9 255.255.255.252
no sh
interface serial 0/0/0
ip add 201.187.89.17 255.255.255.252
no sh
exit
ip route 192.168.0.0 255.255.224.0 201.187.89.2
ip route 192.168.0.0 255.255.224.0 201.187.89.5
ip route 192.168.0.0 255.255.224.0 201.187.89.10
ip route 172.16.0.0 255.240.0.0 201.187.89.2
