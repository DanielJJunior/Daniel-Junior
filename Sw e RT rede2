SW-02

enable
conf t
hostname Sw-02
interface g0/1
switchport mode trunk
switchport trunk allowed vlan 10,20,30,40,50,60,70,80,88,90,100,200
switchport trunk native vlan 88
vlan 50
vlan 60
vlan 70 
vlan 80
interface vlan 88
ip add 10.0.0.254 255.0.0.0
exit
interface f0/1
switchport mode access
switchport access vlan 50
no sh
